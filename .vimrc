syntax on
colors deus
set t_Co=256
set background=dark " Setting dark mode
colorscheme deus
let g:deus_termcolors=256
set noerrorbells
set vb t_vb=
filetype plugin indent on
set tabstop=2
set shiftwidth=2
set expandtab
inoremap <C-BS> <C-w>
inoremap <C-h> <C-w>
set backspace=indent,eol,start
xnoremap p pgvy
noremap <C-m> :make!<CR>
